<div class="question">

  <p id="question_text">{{question}}</p>


  <div id="option_group">
    {{#each choices}}
    <label class="radio">
      <input type="radio" name="question_options" id="option_{{id}}" value="{{id}}">{{description}}
    </label>
    {{/each}}
  </div>

  <button href="" class='submit_answer btn-info'>Submit Answer</button>

</div>
