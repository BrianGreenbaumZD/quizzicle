<div class="question">

  <p id="question_text">{{question}}</p>


  <div id="option_group">
    {{#each choices}}
    <label class="radio">
      <input type="radio" name="question_options" id="option_1" value="{{id}}">{{description}}</input>
    </label>
    {{/each}}
  </div>

  <a href="" class='submit btn-info'>Submit Answer</a>

</div>
