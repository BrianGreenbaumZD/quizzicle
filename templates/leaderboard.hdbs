<div class="leaderboard">

<h4>Leaderboard</h4>

<ol>
  {{#each users}}
    {{#if current_user}}
      <li class="current"><strong>{{user_name}}</strong><span class="score">{{total_score}} points</span></li>
    {{else}}
      <li><strong>{{user_name}}</strong><span class="score">{{total_score}} points</span></li>
    {{/if}}
  {{/each}}
</ol>

<a id="home" href="">Go Home</a>

</div>